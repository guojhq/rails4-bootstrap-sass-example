<div class="row">
  <div class="col-xs-12">
    <div class="page-header">
      <h1>Users</h1>
    </div>

    <table class="table table-striped table-hover">
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Since</th>
        <th>Action</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= link_to 'View', user_path(user) %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>